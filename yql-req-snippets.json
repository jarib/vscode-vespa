			"    \"presentation\" : {",
			"        \"bolding\": false,",
			"        \"format\": \"json\"",
			"    },",
			"    \"trace\": {",
			"      \"level\": 5,",
			"      \"timestamps\": true",
			"    }",
			"}"
		],
		"description": "Full YQL Sample including tracing"
	},
	"trace": {
		"prefix": "yql trace",
		"body": [
			"\"trace\": {",
			"  \"level\": 5,",
			"  \"timestamps\": true",
			"}"
		],
